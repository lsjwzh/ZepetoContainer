<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.support.android.designlibdemo">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:resizeableActivity="false"
    android:theme="@style/Theme.DesignDemo">

    <uses-library
      android:name="com.sec.android.app.multiwindow"
      android:required="false"/>

    <meta-data
      android:name="com.sec.android.support.multiwindow"
      android:value="true"/>

    <meta-data
      android:name="com.sec.android.multiwindow.DEFAULT_SIZE_W"
      android:value="632.0dip"/>

    <meta-data
      android:name="com.sec.android.multiwindow.DEFAULT_SIZE_H"
      android:value="598.0dip"/>

    <meta-data
      android:name="com.sec.android.multiwindow.MINIMUM_SIZE_W"
      android:value="632.0dip"/>

    <meta-data
      android:name="com.sec.android.multiwindow.MINIMUM_SIZE_H"
      android:value="598.0dip"/>

    <meta-data
      android:name="android.max_aspect"
      android:value="1074161280.000000"/>

    <meta-data
      android:name="wx_qlauncher_coop"
      android:value="fun1,fun2"/>

    <meta-data
      android:name="wechat_fun_support"
      android:value="ext1"/>

    <meta-data
      android:name="wechat_ext_api_level"
      android:value="9"/>

    <meta-data
      android:name="SupportedAirMsgVersion"
      android:value="1.0.0"/>

    <activity
      android:name=".MainActivity"
      android:resizeableActivity="false">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.DEFAULT"/>
        <category android:name="android.intent.category.LAUNCHER"/>
        <category
          android:name="android.intent.category.MULTIWINDOW_LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name=".ZepetoDemoActivity"/>


  </application>

</manifest>
